
generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String    @id @default(uuid())
  email     String    @unique
  name      String?
  password  String?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
}